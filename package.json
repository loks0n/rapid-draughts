{
  "name": "rapid-draughts",
  "private": false,
  "version": "0.8.9",
  "license": "MIT",
  "homepage": "https://github.com/loks0n/rapid-draughts#readme",
  "description": "A super speedy, blazing fast, rocket-powered TypeScript draughts/checkers engine.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loks0n/rapid-draughts.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "keywords": [
    "draughts",
    "draughts engine",
    "draughts validation",
    "checkers",
    "checkers engine",
    "draughts validation",
    "boardgame",
    "bitboard",
    "english draughts"
  ],
  "module": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "exports": {
    ".": {
      "import": "./dist/core.js",
      "require": "./dist/core.cjs",
      "types": "./dist/core.d.ts"
    },
    "./english": {
      "import": "./dist/english.js",
      "require": "./dist/english.cjs",
      "types": "./dist/english.d.ts"
    }
  },
  "typesVersions": {
    ">4.0": {
      "index": [
        "./dist/core.d.ts"
      ],
      "english": [
        "./dist/english.d.ts"
      ]
    }
  },
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "publint",
    "build": "tsc && vite build",
    "examples:basic": "vite-node examples/basic.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/ && prettier --check src/",
    "coverage": "vitest run --coverage",
    "release": "release-it"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "prettier": "^2.8.7",
    "publint": "^0.1.11",
    "release-it": "^15.9.3",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-node": "^0.29.7",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.7"
  },
  "dependencies": {
    "long": "^5.2.1"
  }
}
